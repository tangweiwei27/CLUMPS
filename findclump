def FindClumps(texto, K, L, t):
	le=[]
	for na in range (len(texto) - L + 1):
		window= texto[na:na+L]
		counts={}
		for i in range (len(window) - K + 1):
			if window[i:i+K] not in counts:
				counts[window[i:i+K]]= 0
			counts[window[i:i+K]] += 1
		for kmer in counts:
			if counts[kmer]>= t and kmer not in le:
				le.append(kmer)
	print(le)
			
texto= ''
K= 10
L= 28
t= 4

FindClumps(texto, K, L, t)
